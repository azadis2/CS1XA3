{% extends 'base.djhtml' %}

{% block title %}Signup Page{% endblock %}
{% block middle_column %}
    <form method="post" id="signupform" action="{% url 'login:create_view' %}">
        {% csrf_token %}
        {{ signup_form }}
        <input type="submit" value="Submit" />
    </form>
    {% if create_failed %}
        <p style="color:red">Incorrect username or password</p>
    {% endif %}
{% endblock %}
